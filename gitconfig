  name = Roshan Choxi
  email = roshan.choxi@gmail.com
[core]
  excludesfile = /Users/rosh/.gitignore
  editor = vim
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  ci = commit
  co = checkout
  logl = log --pretty=oneline --abbrev-commit --decorate
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %f %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  unadd = update-index --force-remove
[github]
  user = choxi
[branch]
  autosetupmerge = true
[apply]
  whitespace = nowarn
[push]
  default = tracking
